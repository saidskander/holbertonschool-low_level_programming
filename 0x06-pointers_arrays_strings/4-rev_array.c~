#include "holberton.h"
#include <stdio.h>
/**
 * main - check the code for Holberton School students.
 * @a: an array of integers
 * @n: the number of elements to swap
 *
 * Return: nothing.
 */
void print_array(int *a, int n)
{
int x;
int i;
for (i = 0; n = n - 1 && i <= n; i++)
{
x = a[i];
a[i] = a[n];
a[n] = x;
n--;
}
}
